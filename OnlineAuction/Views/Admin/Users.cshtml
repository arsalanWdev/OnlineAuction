@model IEnumerable<OnlineAuction.Areas.Identity.Data.ApplicationUser>

    <div class="container">
        <br /><br />
<h2>Manage Users</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>User ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Password</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>*****</td>
                <td>
                    <a asp-action="EditUser" asp-route-id="@user.Id" class="btn btn-warning">Edit</a>
                    <a asp-action="DeleteUser" asp-route-id="@user.Id" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
</div>